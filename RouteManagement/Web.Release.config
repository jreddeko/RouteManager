<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator
    finds an attribute "name" that has a value of "MyDB".

    <connectionStrings>
      <add name="MyDB"
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->


  <connectionStrings>
    <add name="EdiObjectContext"
         connectionString="metadata=res://*/EdiOrdering.EdiObjectContext.csdl|res://*/EdiOrdering.EdiObjectContext.ssdl|res://*/EdiOrdering.EdiObjectContext.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=DBSERVER;initial catalog=EdiOrdering;persist security info=True;user id=empower;password=empower;MultipleActiveResultSets=True;App=EntityFramework&quot;"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="AscObjectContext"
         connectionString="metadata=res://*/AscTrac.AscObjectContext.csdl|res://*/AscTrac.AscObjectContext.ssdl|res://*/AscTrac.AscObjectContext.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=DBSERVER;initial catalog=ASCTrac;user id=empower;password=empower;MultipleActiveResultSets=True;App=EntityFramework&quot;"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="GpObjectContext"
         connectionString="metadata=res://*/GreatPlains.GPObjectContext.csdl|res://*/GreatPlains.GPObjectContext.ssdl|res://*/GreatPlains.GPObjectContext.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=DBSERVER;initial catalog=WDDC;user id=empower;password=empower;MultipleActiveResultSets=True;App=EntityFramework&quot;"
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <system.web>
    <compilation debug="false" targetFramework="4.0.0" xdt:Transform="SetAttributes(debug, targetFramework)"/>
    <!--
      In the example below, the "Replace" transform will replace the entire
      <customErrors> section of your Web.config file.
      Note that because there is only one customErrors section under the
      <system.web> node, there is no need to use the "xdt:Locator" attribute.

      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"  xdt:Transform="SetAttributes(runAllManagedModulesForAllRequests)"/>
  </system.webServer>
</configuration>
