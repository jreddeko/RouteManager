<html>
<head>
    <title>Route Mangement</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/001-road.png" defer />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" defer />
    <link rel="stylesheet" href="~/Content/font-awesome-4.7.0/css/font-awesome.css" defer>
    <link rel="stylesheet" href="~/Content/paper-styles.css" defer />
    <link rel="stylesheet" href="~/Content/branding.css" defer />
    <link rel="stylesheet" href="~/Content/Site.css" defer/>
    @RenderSection("datetimepickerstyles", false)

    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script> 
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous" defer></script>
    <script src="http://spin.js.org/spin.min.js" defer></script>
    <script src="http://malsup.github.io/jquery.blockUI.js" defer></script>
    <script src="~/Scripts/spinner.js" defer></script>
    @RenderSection("vueHomePage", false)
</head>
<body>
    <nav class="navbar navbar-default">
        @Html.Partial("_HeaderPartial")
    </nav>
    <div class="container-fluid paper-font-body1">
        <div class="alert alert-success" id="alert-sucess" style="display:none;">
            <strong>Success!</strong> Route successfully saved.
        </div>
        <div class="alert alert-warning" id="alert-warning" style="display:none;">
            <strong>Failure!</strong> No information has been changed.
        </div>
        <div class="alert alert-danger" id="alert-danger" style="display:none;">
            <strong>Failure!</strong> No information was saved.
        </div>
        @RenderBody()
    </div>
    @RenderSection("datetimepickerscripts", false)
    @RenderSection("vueMemberPage", false)
</body>
</html>
